<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean 	id="initDestroyBean" 
			class="com.itwill1.bean.attr.InitDestroyBean"
			init-method="myInit"
			destroy-method="myDestroy"
			/>
	<bean id="lazyInitBean" 
	class="com.itwill1.bean.attr.LazyInitBean"
	lazy-init="true"/>
	
	<bean id="singletonBean" 
		  class="com.itwill1.bean.attr.SingletonScopeBean"
		  scope="singleton"/>
	<bean id="prototypeBean" 
		  class="com.itwill1.bean.attr.PrototypeScopeBean"
		  scope="prototype"/>	  
	<bean 	id="dependsOnB" 
			depends-on="dependsOnA" 
			class="com.itwill1.bean.attr.DependsOnB"/>
	<bean id="dependsOnA" class="com.itwill1.bean.attr.DependsOnA"/>
		  
</beans>











